{
  "name": "edsheet",
  "version": "1.0.0",
  "description": "editable table component, supported react and vue",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/JE-lee/worktable.git"
  },
  "scripts": {
    "build": "nx run-many --target=build",
    "clean": "pnpm --stream -r clean",
    "test": "pnpm --stream -r test",
    "build:core": "nx build @edsheet/core",
    "dev:element-ui": "npm run build:core && concurrently \"nx dev:build @edsheet/core\" \"nx docs:dev @edsheet/element-ui\"",
    "ci:publish": "pnpm publish -r -no-git-checks",
    "version": "npx changeset version"
  },
  "keywords": [
    "formily",
    "inline",
    "editable",
    "table",
    "element-ui",
    "ant-design-vue"
  ],
  "author": "jelee",
  "license": "MIT",
  "dependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@changesets/cli": "^2.26.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.9.0",
    "tsc-alias": "^1.8.5",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/jest": "^29.2.3",
    "babel-jest": "^29.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nx": "^15.4.2",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.6.0",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">17.0"
  }
}
